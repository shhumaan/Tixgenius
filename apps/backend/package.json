{
  "name": "@tixgenius/backend",
  "version": "1.0.0",
  "private": true,
  "description": "TixGenius Backend Service",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsc -w -p tsconfig.json\" \"nodemon dist/index.js\"",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "bcrypt": "^5.1.1",
    "connect-redis": "^8.0.2",
    "express": "^5.1.0",
    "express-session": "^1.18.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "winston": "^3.13.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tixgenius/eslint-config": "file:../../packages/eslint-config",
    "@tixgenius/typescript-config": "file:../../packages/typescript-config",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.14.1",
    "concurrently": "^9.1.2",
    "eslint": "^9.24.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
